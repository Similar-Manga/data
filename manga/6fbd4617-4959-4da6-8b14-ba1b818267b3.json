  "title": {
   "en": "Demon in Mount Hua"
  },
  "altTitles": [
   {
    "ko": "마인화산"
   },
   {
    "ru": "Демон с горы Хуа"
   },
   {
    "ko": "마IN화산"
   },
   {
    "zh-hk": "魔in華山"
   }
