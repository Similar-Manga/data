  "title": {
   "en": "My oc is Fine"
  },
  "altTitles": [
   {
    "zh": "我的oc成精了"
   },
   {
    "zh-ro": "Wo De oc Cheng Jingle"
   },
   {
    "zh-ro": "Wǒ De oc Chéng Jīngle"
   }
