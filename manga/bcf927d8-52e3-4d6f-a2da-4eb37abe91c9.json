  "title": {
   "en": "Quest Supremacy"
  },
  "altTitles": [
   {
    "ko": "퀘스트지상주의"
   },
   {
    "ko-ro": "Quest Jisangjuui"
   },
   {
    "en": "QUESTISM"
   },
   {
    "ru": "Квестовое превосходство"
   }
