  "title": {
   "en": "Higurashi no Naku Koro ni Meguri"
  },
  "altTitles": [
   {
    "ja": "ひぐらしのなく頃に 巡"
   },
   {
    "ru": "Когда Плачут Цикады: Цикл"
   }
