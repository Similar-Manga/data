  "title": {
   "en": "For Snow White"
  },
  "altTitles": [
   {
    "ko": "백설을 위하여"
   },
   {
    "ko-ro": "Baekseoreul Wihayeo"
   },
   {
    "ja": "白雪のために"
   }
