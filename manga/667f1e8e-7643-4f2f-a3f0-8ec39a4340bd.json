  "title": {
   "en": "Don't Cry Maou-chan"
  },
  "altTitles": [
   {
    "en": "Don't cry, little Demon King"
   },
   {
    "ja-ro": "Nakanaide Maou-chan"
   },
   {
    "ja": "泣かないで 魔王ちゃん"
