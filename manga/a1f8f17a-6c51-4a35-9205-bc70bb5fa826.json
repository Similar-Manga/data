  "altTitles": [
   {
    "ja": "つれないほど青くて あざといくらいに赤い"
   },
   {
    "ru": "Холодно-синий и коварно-красный"
   },
   {
    "vi": "Xanh của Lạnh Lùng. Đỏ của Xảo Quyệt."
   }
