  "title": {
   "en": "Absolute Martial Arts"
  },
  "altTitles": [
   {
    "ko": "절대무신"
   },
   {
    "ja": "絶代武神～伝説の始まり～"
   },
   {
    "ru": "Абсолютный воин"
   }
